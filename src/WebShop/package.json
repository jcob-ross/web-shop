{
  "name": "angular2-web-shop",
  "version": "0.0.0",
  "description": "SPA Web Shop",
  "keywords": [
  ],
  "author": "Rossler Jakub <rossler.jakub@email.cz>",
  "homepage": "https://github.com/jcob-ross/web-shop",
  "license": "MIT",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",

    "typings-install": "npm run typings -- install",

    "clean:dev": "npm run rimraf -- doc/client coverage/client",
    "clean:dist": "npm run rimraf -- wwwroot/spa",

    "watch": "npm run watch:dev",
      "watch:dev": "npm run build:dev -- --watch",
      "watch:test": "npm run test -- --auto-watch --no-single-run",
      "watch:prod": "npm run build:prod -- --watch",

    "build": "npm run build:dev",
        "prebuild:dev": "npm run clean:dist",
      "build:dev": "webpack --config Client/config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
        "prebuild:prod": "npm run clean:dist",
      "build:prod": "webpack --config Client/config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",

    "lint": "npm run tslint \"src/**/*.ts\"",

    "test": "karma start",

    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",

    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "@angular/http": "2.0.0-rc.4",
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",

    "ie-shim": "^0.1.0",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12",

    "ng2-bootstrap": "1.0.23"
  },
  "devDependencies": {
    "parse5": "^1.5.1",
    "rimraf": "^2.5.2",

    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",

    "remap-istanbul": "^0.6.3",

    "codelyzer": "~0.0.21",

    "tslint": "^3.7.1",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.1",
    "typedoc": "^0.4.4",
    "typescript": "~1.8.10",
    "typings": "~1.3.0",

    "node-sass": "^3.8.0",    
    "sass-loader": "^3.2.3",
    "postcss-loader": "^0.9.1",
    "null-loader": "0.1.1",

    "extract-text-webpack-plugin": "^1.0.1",
    "awesome-typescript-loader": "1.1.1",
    "angular2-template-loader": "^0.4.0",
    "tslint-loader": "^2.1.3",
    "url-loader": "^0.5.7",
    "style-loader": "^0.13.1",
    "raw-loader": "0.5.1",
    "source-map-loader": "^0.1.5",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "es6-promise-loader": "^1.0.1",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "to-string-loader": "^1.1.4",
    "compression-webpack-plugin": "^0.3.1",
    
    "copy-webpack-plugin": "^3.0.1",

    "jasmine-core": "2.4.1",

    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",

